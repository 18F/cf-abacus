{
  "name": "cf-abacus",
  "description": "Abacus usage metering and aggregation service",
  "readme": "README.md",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "etc/versions && npm install npm@2.11.3 && node_modules/.bin/npm install && npm test",
    "start": "bin/start",
    "stop": "bin/stop",
    "demo": "bin/demo",
    "cfpush": "bin/cfpush",
    "cfstart": "bin/cfstart",
    "cfstop": "bin/cfstop",
    "doc": "cat README.md | mddoc >Abacus-README.html",
    "cachedinstall": "npm --cache-min 9999999 install npm@2.11.3 && node_modules/.bin/npm --cache-min 9999999 install",
    "reinstall": "npm run uninstall && npm install npm@2.11.3 && node_modules/.bin/npm install",
    "uninstall": "rm -rf node_modules",
    "lint": "etc/foreach cf-abacus npm run lint",
    "test": "etc/foreach cf-abacus npm test",
    "perf": "bin/perf"
  },
  "dependencies": {
    "cf-abacus-eslint": "file:tools/eslint",
    "cf-abacus-mddoc": "file:tools/mddoc",
    "cf-abacus-mocha": "file:tools/mocha",
    "cf-abacus-packapp": "file:tools/packapp",
    "cf-abacus-service-config": "file:lib/config/service",
    "cf-abacus-usage-accumulator": "file:lib/aggregation/accumulator",
    "cf-abacus-usage-aggregator": "file:lib/aggregation/aggregator",
    "cf-abacus-instance-launcher": "file:lib/aggregation/launcher",
    "cf-abacus-aggregation-db": "file:lib/aggregation/db",
    "cf-abacus-usage-reporting": "file:lib/aggregation/reporting",
    "cf-abacus-usage-collector": "file:lib/metering/collector",
    "cf-abacus-usage-meter": "file:lib/metering/meter",
    "cf-abacus-metering-db": "file:lib/metering/db",
    "cf-abacus-metering-schemas": "file:lib/metering/schemas",
    "cf-abacus-usage-rate": "file:lib/rating/rate",
    "cf-abacus-account-stub": "file:lib/stubs/account",
    "cf-abacus-provisioning-stub": "file:lib/stubs/provisioning",
    "cf-abacus-batch": "file:lib/utils/batch",
    "cf-abacus-breaker": "file:lib/utils/breaker",
    "cf-abacus-cluster": "file:lib/utils/cluster",
    "cf-abacus-dbclient": "file:lib/utils/dbclient",
    "cf-abacus-dbserver": "file:lib/utils/dbserver",
    "cf-abacus-debug": "file:lib/utils/debug",
    "cf-abacus-es6runner": "file:lib/utils/es6runner",
    "cf-abacus-events": "file:lib/utils/events",
    "cf-abacus-express": "file:lib/utils/express",
    "cf-abacus-hystrix": "file:lib/utils/hystrix",
    "cf-abacus-lock": "file:lib/utils/lock",
    "cf-abacus-partition": "file:lib/utils/partition",
    "cf-abacus-perf": "file:lib/utils/perf",
    "cf-abacus-request": "file:lib/utils/request",
    "cf-abacus-retry": "file:lib/utils/retry",
    "cf-abacus-router": "file:lib/utils/router",
    "cf-abacus-schema": "file:lib/utils/schema",
    "cf-abacus-seqid": "file:lib/utils/seqid",
    "cf-abacus-throttle": "file:lib/utils/throttle",
    "cf-abacus-transform": "file:lib/utils/transform",
    "cf-abacus-urienv": "file:lib/utils/urienv",
    "cf-abacus-vcapenv": "file:lib/utils/vcapenv",
    "cf-abacus-webapp": "file:lib/utils/webapp",
    "cf-abacus-yieldable": "file:lib/utils/yieldable",
    "cf-abacus-perf-test": "file:test/perf",
    "cf-abacus-demo-client": "file:demo/client",
    "cf-abacus-doc": "file:doc",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.0.0"
  },
  "repository": {}
